{
    "name": "spoonity/common",
    "description": "Simple library of common project classes and services packaged for re-use.",
    "type": "library",
    "authors": [
        {
            "name": "brandin",
            "email": "brandin@spoonity.com"
        }
    ],
    "license": "proprietary",
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "ext-curl": "*",
        "ext-pdo": "*",
        "symfony/framework-bundle": "^5.0.0",
        "twig/twig": "^2.0",
        "symfony/yaml": "^5.0",
        "symfony/validator": "^5.0",
        "symfony/form": "^5.0",
        "symfony/twig-bundle": "^5.0",
        "twig/extra-bundle": "^3.0",
        "twig/cssinliner-extra": "^3.0",
        "symfony/console": "^5.4",
        "symfony/orm-pack": "^2.3"
    },
    "config": {
        "platform": {
            "php": "7.4"
        }
    },
    "autoload": {
        "psr-4": { "Spoonity\\Common\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "stable"
}
